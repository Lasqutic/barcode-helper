# Ignore node_modules directory, which contains installed dependencies
node_modules/

# Ignore build directories (commonly used for compiled or bundled files)
dist/
build/

# Ignore log files and temporary files
*.log
*.tmp
*.temp
*.cache

# Ignore environment variable files (they may contain sensitive information)
.env
.env.local
.env.*.local

# Ignore IDE and editor specific files/settings
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# Ignore operating system specific files
.DS_Store       # macOS
Thumbs.db       # Windows

# Ignore npm debug files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore coverage reports generated by testing frameworks
coverage/

# Ignore compiled and minified files (often generated by build tools like Webpack, Gulp, etc.)
*.min.js
*.min.css

# Ignore SASS/SCSS cache files
.sass-cache/

# Ignore JavaScript map files
*.map

# Ignore optional lock files for dependency managers
package-lock.json
yarn.lock
